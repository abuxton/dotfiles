# Brew_profile for addiitional paths etc from Brew/Cask etc
export PATH="/opt/homebrew/sbin/opt/homebrew/bin:/usr/local/opt/helm@2/bin:/usr/local/opt/coreutils/libexec/gnubin:$PATH"

alias brew-dump='brew bundle dump --describe'

